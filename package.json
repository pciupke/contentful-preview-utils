  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "contentful": "^9.3.2",
    "contentful-management": "^10.29.1",
    "npmignore": "^0.3.0"
  },
  "dependencies": {
    "fast-safe-stringify": "^2.1.1",
